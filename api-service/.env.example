# Environment variables for API service
NODE_ENV=production
PORT=3000

# Database configuration
MONGODB_URI=mongodb://mongodb:27017/multiservice
DB_NAME=multiservice

# Redis configuration
REDIS_URL=redis://redis:6379

# Security
SESSION_SECRET=your-super-secret-session-key-change-in-production

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Cache settings
CACHE_TTL=60

# Logging
LOG_LEVEL=info